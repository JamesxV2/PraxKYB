    asu_maroon = (0x8C, 0x1D, 0x40)

Kod:
#!/bin/python
from pwn import *

with open("example.cimg", "wb") as file:
    file.write(b"cIMG") 
    file.write((2).to_bytes(4, "little"))
    file.write((73).to_bytes(8, "little")) 
    file.write((20).to_bytes(2, "little"))

    r = 0x8C
    g = 0x1D
    b = 0x40
    
    for _ in range(73 * 20):
        file.write(bytes([r, g, b, ord('X')]))
