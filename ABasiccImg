Parametre:
    header = file.read1(18)
    assert len(header) == 18, "ERROR: Failed to read header!"
    assert header[:4] == b"cIMG", "ERROR: Invalid magic number!"
    assert int.from_bytes(header[4:8], "little") == 2, "ERROR: Invalid version!"
    width = int.from_bytes(header[8:16], "little")
    assert width == 73, "ERROR: Incorrect width!"
    height = int.from_bytes(header[16:18], "little")
    assert height == 20, "ERROR: Incorrect height!"
    data = file.read1(width * height * 4)
    assert len(data) == width * height * 4, "ERROR: Failed to read data!"
